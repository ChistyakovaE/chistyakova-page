@startuml
class "Система записи к диетологу" {
  пациент
  диетолог
  время
} 
class Пациент {
  номер телефона
  ФИО
}
class "Приложение" {
  название
  дата выпуска
Место работы
}
class "ГОСТы" {
  номер
  описание
}
class "ФЗ" {
  номер
  описание
}
class "Нормативно-справочная информация" {
  фио диетолога
  должность
  стаж
}
"Система записи к диетологу" *-- "Пациент"
"Система записи к диетологу" *-- "Приложение"
"Приложение" *-- "ГОСТы"
"Приложение" *-- "ФЗ"
"Система записи к диетологу" *-- "Нормативно-справочная информация"
